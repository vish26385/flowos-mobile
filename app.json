{
  "expo": {
    "name": "FlowOS",
    "slug": "flowos-mobile",
    "scheme": "flowos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#7b61ff"
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "extra": {
      "eas": {
        "projectId": "ef4d4f42-fbb8-4084-b256-fb6fd8077519"
      },
      "API_URL": "https://flowosapi-f5gxcvgkenfpezaa.canadacentral-01.azurewebsites.net/api"
    },
    "android": {
      "package": "com.anonymous.flowosmobile"
    }
  }
}
